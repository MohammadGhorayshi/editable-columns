{
    "name": "mohammadghorayshi/editable-columns",
    "description": "Add input columns to your tables, to let your admins edit values right then and there.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ghorayshi",
            "email": "ghmohammad99@gmail.com"
        }
    ],
    "homepage": "https://github.com/MohammadGhorayshi/editable-columns",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel"
    ],
    "require": {
        "backpack/crud": "^5.3.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\EditableColumns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\EditableColumns\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backpack\\EditableColumns\\AddonServiceProvider"
            ]
        }
    }
}
